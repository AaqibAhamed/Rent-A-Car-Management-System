<Window x:Class="AutoDealership.Form.ViewShowroom"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
       xmlns:xctk="http://schemas.xceed.com/wpf/xaml/toolkit"
        xmlns:System="clr-namespace:System;assembly=mscorlib"
        Title="ViewEmployee" Height="305.515" Width="401.47" Loaded="Window_Loaded">
    <Grid Background="#FFC0C8C9">
        <DataGrid AutoGenerateColumns="False" Name="SRGrid">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding ShowRName}" Width="130" Header="Showroom Name"/>
                <DataGridTextColumn Binding="{Binding SRContectNumber}" Width="130" Header="Contact Number"/>
                <DataGridTextColumn Binding="{Binding CityName}" Width="130" Header="City"/>
            </DataGrid.Columns>
        </DataGrid>
    </Grid>
</Window>
